{
  "name": "backend",
  "version": "0.0.1",
  "description": "API server for testing",
  "scripts": {
    "start": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js ./dist/index.js",
    "watch": "cross-env NODE_ENV=development DEBUG=server nodemon --exec ts-node -r ./tsconfig-paths-bootstrap.js --files src/index.ts -e ts",
    "dev": "cross-env NODE_ENV=development DEBUG=server,error nodemon --exec ts-node -r ./tsconfig-paths-bootstrap.js  --files src/index.ts -e ts",
    "build": "tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "coverage": "jest --coverage"
  },
  "main": "./dist/index.js",
  "author": "khyun-kim",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    }
  },
  "dependencies": {
    "@decorators/di": "^1.0.2",
    "@decorators/express": "^2.4.0",
    "@typegoose/typegoose": "^7.4.2",
    "aws-sdk": "^2.780.0",
    "axios": "^0.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.8",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.1",
    "@types/mongoose-auto-increment": "^5.0.33",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.8",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.0.3"
  }
}
